    <servlet>
        <servlet-name>SyncServlet</servlet-name>
        <servlet-class>web.Sync.SyncServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Statictics</servlet-name>
        <servlet-class>web.Statictics.DataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Fugure</servlet-name>
        <servlet-class>web.Statictics.FugureServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Route</servlet-name>
        <servlet-class>web.Entety.RouteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YearData</servlet-name>
        <servlet-class>web.Statictics.YearData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonthData</servlet-name>
        <servlet-class>web.Statictics.MonthData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SyncServlet</servlet-name>
        <url-pattern>/sync</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Statictics</servlet-name>
        <url-pattern>/statictics/data</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Fugure</servlet-name>
        <url-pattern>/statictics/fugure</url-pattern>
