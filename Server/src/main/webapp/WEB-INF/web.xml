    <servlet-mapping>
        <servlet-name>Route</servlet-name>
        <url-pattern>/entety/route</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YearData</servlet-name>
        <url-pattern>/statictics/years</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonthData</servlet-name>
        <url-pattern>/statictics/month</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
